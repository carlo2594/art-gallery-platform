{
  "name": "galeria-del-ox-node",
  "version": "1.0.0",
  "description": "Backend for Galer√≠a del Ox using Express, Mongoose, and Pug.",
  "main": "server.js",
  "scripts": {
    "dev": "node --watch server.js",
    "start": "node server.js",
    "start:prod": "set NODE_ENV=production&& node server.js",
    "db:clean": "node scripts/cleanDatabase.js",
    "db:seed": "node scripts/seedTestData.js",
    "db:reset": "npm run db:clean && npm run db:seed",
    "views:refresh": "node scripts/refreshArtworkViews.js"
  },
  "keywords": [
    "galeria",
